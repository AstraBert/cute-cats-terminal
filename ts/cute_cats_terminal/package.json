{
  "name": "cute-cats-terminal",
  "version": "0.3.1",
  "description": "A package to print cute cats to terminal",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {
    "cute-cats-terminal": "./bin/run"
  },
  "directories": {
    "lib": "src",
    "bin": "bin"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint ./src/",
    "compile": "tsc",
    "start": "ts-node ./bin/run.ts",
    "format": "prettier --write ./src/",
    "build": "tsup-node",
    "build:watch": "tsup-node --watch",
    "dev": "pnpm exec tsx --watch src/index.ts",
    "version": "cd .. && pnpm changeset version",
    "release": "pnpm run build && cd .. && pnpm changeset publish"
  },
  "keywords": [
    "cats",
    "ascii-art",
    "cute",
    "terminal"
  ],
  "author": "Clelia Astra Bertelli",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "picocolors": "^1.1.1",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
